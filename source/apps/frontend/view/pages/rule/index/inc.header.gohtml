<!-- Modal -->
<div class="modal fade" id="createModal" tabindex="-1" role="dialog"  data-bs-keyboard="false" data-bs-backdrop="static"
     aria-labelledby="createModalLabel"
     aria-hidden="true">
    <div class="modal-create modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header border-0 pb-0">
                {{/*                <h5 class="modal-title" id="createModalLabel">Modal title</h5>*/}}
                <button type="button" class="close modal-close" data-bs-dismiss="modal" aria-label="Close">
                    <i class="fa fa-close" style="font-size:20px;color: #000000"></i>
                </button>
            </div>
            <div class="modal-body pt-0">
                <div class="modal-title text-center" style="font-size: 20px">Create a Rule</div>
                <div class="modal-subheader text-center mt-2">Choose one rule type from below</div>
                <div class="row mt-3" style="padding-left: 25px">
                    <div class="box-rule col-md-4 card" data-type="blocking">
                        <div class="card-header card-rule border-0 p-0">
                            <i class="fa fa-pencil" style="font-size:16px;color: #000000"></i> <span class="ms-1">Ad Quality</span></div>
                        <div class="card-text pt-2">
                            <span style="font-size: 13px">Block unwanted ads by advertisers domains, creative IDâ€¦.</span>
                        </div>
                    </div>
                    <div class="box-rule col-md-4 card" data-type="floor">
                        <div class="card-header card-rule border-0 p-0">
                            <i class="fa fa-dollar" style="font-size:16px;color: #000000"></i> <span class="ms-1">Floor</span></div>
                        <div class="card-text pt-2">
                            <span style="font-size: 13px">Set a dynamic or hard floor price</span>
                        </div>
                    </div>
                    <div class="box-rule col-md-4 card" data-type="blocked-page">
                        <div class="card-header card-rule border-0 p-0">
                            <i class="fa fa-ban" style="font-size:16px;color: #000000"></i>  <span class="ms-1">Blocked Page</span></div>
                        <div class="card-text pt-2">
                            <span style="font-size: 13px">Block ads from showing on some unwanted pages</span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-small btn-white modal-close" data-bs-dismiss="modal" aria-label="Close">Cancel</button>
                <button class="btn btn-small btn-danger btn-create" disabled>Create</button>
            </div>
        </div>
    </div>
</div>

<style>
    .box-rule.selected{
        border: 1px solid #000000!important;
    }

    .card-rule{
        height: auto;
        font-size: 15px;
        font-weight: bold;
        color: #475470
    }

    .modal-create{
        max-width: 600px;
    }

    .box-rule{
        cursor: pointer;
        min-width: 166px;
        max-width: 166px;
        height: 150px;
        border-radius: 3px;
        border: 1px solid #d8dde6;
        margin-right: 20px;
        padding: 16px;
        margin-bottom: 20px;
        color: #8695b1;
    }
    a:not(.btn):focus {
        color: #ffffff !important;
    }
</style>