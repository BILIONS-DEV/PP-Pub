                                    </div>
                                    {{/*Target*/}}
                                </div>
                            </div>
                        </div>
                        {{/*Targeting Card*/}}
                    </fieldset>

                    <div id="SettingBox"
                         class="card rounded-0 shadow-dark-80 border border-gray-50 mb-5 box_type_google_hide {{if eq .Row.TableLineItem.ServerType.Int 2}}d-none{{end}}">
                        <div class="d-flex align-items-center bg-dominant px-4 toggle-collapse">
                            <h5 class="card-header-title font-weight-semibold m-2">{{$DemandLang.DemandPartners}}</h5>
                            <div class="ms-auto pe-md-2">
                                <div class="export-dropdown">
                                    <a aria-controls="collapseSettingBox" data-bs-target="#collapseSettingBox"
                                       data-bs-toggle="collapse"
                                       aria-expanded="{{if (InArray "collapseSettingBox" $.ListBoxCollapse false)}}false{{else}}true{{end}}"
                                       class="btn btn-link px-3 {{if (InArray "collapseSettingBox" $.ListBoxCollapse false)}}{{else}}collapsed{{end}}"
                                       href="javascript:void(0);">
                                        <svg class="ms-2" xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                             viewBox="0 0 13 13">
                                            <rect data-name="Icons/Tabler/Chevron Down background" width="16"
                                                  height="16" fill="none"></rect>
                                            <path d="M.214.212a.738.738,0,0,1,.952-.07l.082.07L7.1,5.989a.716.716,0,0,1,.071.94L7.1,7.011l-5.85,5.778a.738.738,0,0,1-1.034,0,.716.716,0,0,1-.071-.94l.071-.081L5.547,6.5.214,1.233A.716.716,0,0,1,.143.293Z"
                                                  transform="translate(13 3.25) rotate(90)" fill="#1e1e1e"></path>
                                        </svg>
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div id="collapseSettingBox"
                             class="collapse {{if (InArray "collapseSettingBox" $.ListBoxCollapse false)}}{{else}}show{{end}}"
                             data-bs-parent="#SettingBox">
                            <div class="card-body px-5 py-for-card">

                                {{/*                                <div class="row my-4" id="select_bidder_box">*/}}
                                {{/*                                    <div class="d-flex align-items-center">*/}}
                                {{/*                                        <label class="col-form-label form-label form-label-lg w-input-group">*/}}
                                {{/*                                            {{$DemandLang.SelectBidder}}*/}}
                                {{/*                                        </label>*/}}
                                {{/*                                        <select id="select-bidder" class="form-select select2 w-50-custom"*/}}
                                {{/*                                                data-placeholder="{{$DemandLang.SelectBidderPlaceholder}}"*/}}
                                {{/*                                                style="text-transform: capitalize">*/}}
                                {{/*                                            <option></option>*/}}
                                {{/*                                            {{range .Bidders}}*/}}
                                {{/*                                                <option value="{{.TableBidder.Id}}">{{.TableBidder.BidderCode}}</option>*/}}
                                {{/*                                            {{end}}*/}}
                                {{/*                                        </select>*/}}
                                {{/*                                        <span class="form-text ms-2 w-auto invalid-feedback"></span>*/}}
                                {{/*                                    </div>*/}}
                                {{/*                                </div>*/}}
                                {{/*Select Bidder*/}}

                                <div id="bidder-params">
                                    {{if .BidderParams}}
                                        {{range $i, $v := .BidderParams}}
                                            <div class="box-c" id="bidder_{{$v.BidderId}}_{{$v.BidderIndex}}">
                                                <hr class="mt-5 hr_custom bg-gray-400">
                                                <div class="bidder-box" data-id="{{$v.BidderId}}"
                                                     data-name="{{$v.BidderName}}" data-index="{{$v.BidderIndex}}">
                                                    <div class="row my-4">
                                                        <div class="d-flex align-items-center bidder-name">
                                                            <label class="col-form-label form-label form-label-lg w-input-group text-uppercase">
                                                                {{$v.BidderName}}
                                                            </label>
                                                            <div class="d">
                                                                <a class="btn p-1 ms-2 rm_c disabled"
                                                                   data-id="{{$v.BidderId}}"
                                                                   data-index="{{$v.BidderIndex}}">
                                                                    <svg xmlns="http://www.w3.org/2000/svg" width="15"
                                                                         height="15" fill="currentColor"
                                                                         class="bi bi-x-lg" viewBox="0 0 16 16">
                                                                        <path d="M1.293 1.293a1 1 0 0 1 1.414 0L8 6.586l5.293-5.293a1 1 0 1 1 1.414 1.414L9.414 8l5.293 5.293a1 1 0 0 1-1.414 1.414L8 9.414l-5.293 5.293a1 1 0 0 1-1.414-1.414L6.586 8 1.293 2.707a1 1 0 0 1 0-1.414z"></path>
                                                                    </svg>
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row my-4">
                                                        <div class="d-flex">
                                                            <label class="col-form-label form-label form-label-lg w-input-group"
                                                                   for="type_{{$v.BidderType.Int}}_{{$v.BidderIndex}}">
                                                                Type
                                                            </label>
                                                            <select disabled
                                                                    id="type_{{$v.BidderId}}_{{$v.BidderIndex}}"
                                                                    name="type"
                                                                    data-id="{{$v.BidderId}}"
                                                                    data-index="{{$v.BidderIndex}}"
                                                                    data-name="{{$v.BidderName}}"
                                                                    class="form-select w-50-custom type-bidder b-t-{{$v.BidderId}}">
                                                                <option value="1"
                                                                        {{if eq $v.BidderType.Int 1}}selected{{end}}>
                                                                    Prebid Client
                                                                </option>
                                                                {{if ne $v.BidderTemplate 2}}
                                                                    <option value="2"
                                                                            {{if eq $v.BidderType.Int 2}}selected{{end}}>
                                                                        Prebid Server (S2S)
                                                                    </option>
                                                                {{end}}
                                                            </select>
                                                        </div>
                                                    </div>
                                                    {{/*Type*/}}

                                                    <div class="list_param">
                                                        {{range $v.BidderParams}}
                                                            <div class="row my-4 box-param">
                                                                <div class="d-flex align-items-center">
                                                                    <label class="col-form-label form-label form-label-lg w-input-group"
                                                                           for="{{$v.BidderId}}-{{.Param.Name}}-{{$v.BidderIndex}}"
                                                                           style="text-transform: capitalize">
                                                                        {{.Param.Name}}
                                                                    </label>
                                                                    {{if eq .Param.Type "boolean"}}
                                                                        <select id="{{$v.BidderId}}-{{.Param.Name}}-{{$v.BidderIndex}}"
                                                                                disabled
                                                                                class="form-select w-50-custom param_value"
                                                                                data-type="{{.Param.Type}}"
                                                                                data-name="{{.Param.Name}}">
                                                                            <option value="true"
                                                                                    {{if eq .Param.Value "true"}}selected{{end}}>
                                                                                true
                                                                            </option>
                                                                            <option value="false"
                                                                                    {{if eq .Param.Value "false"}}selected{{end}}>
                                                                                false
                                                                            </option>
                                                                        </select>
                                                                    {{else}}
                                                                        <input type="{{if eq .Param.Type "int"}}number{{else}}text{{end}}"
                                                                               class="form-control w-50-custom param_value"
                                                                               id="{{$v.BidderId}}-{{.Param.Name}}-{{$v.BidderIndex}}"
                                                                               value="{{.Param.Value}}"
                                                                               placeholder="{{.Param.Type}}"
                                                                               data-type="{{.Param.Type}}"
                                                                               data-name="{{.Param.Name}}" disabled>
                                                                    {{end}}
                                                                    {{if ne .IsRequired true}}
                                                                        <div class="ms-2 ps-0 d-flex align-items-center">
                                                                            <button type="button" disabled
                                                                                    class="btn btn-outline-danger btn-sm px-2 rounded-2 btn-remove-param d-none">
                                                                                <svg xmlns="http://www.w3.org/2000/svg"
                                                                                     width="16" height="16"
                                                                                     fill="currentColor"
                                                                                     class="bi bi-trash"
                                                                                     viewBox="0 0 16 16">
                                                                                    <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z"></path>
                                                                                    <path fill-rule="evenodd"
                                                                                          d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z"></path>
                                                                                </svg>
                                                                            </button>
                                                                        </div>
