                                    <div class="row my-4">
                                        <input type="hidden" id="id-line-item" value="{{.Row.TableLineItem.Id}}"
                                               name="id"/>
                                        <div class="d-flex">
                                            <label class="col-form-label form-label form-label-lg w-input-group">
                                                {{$DemandLang.Target}}
                                            </label>
                                            <div class="col-sm-8">
                                                <div id="target"
                                                     class="accordion muze-collapes border border-bottom-0 border-gray-50">

                                                    {{template "line-item/view/target.domain" .}}
                                                    {{/*Target Domains*/}}

                                                    {{template "line-item/view/target.format" .}}
                                                    {{/*Target Format*/}}

                                                    {{template "line-item/view/target.size" .}}
                                                    {{/*Target Size*/}}

                                                    {{template "line-item/view/target.adtag" .}}
                                                    {{/*Target Ad Tag*/}}

                                                    {{template "line-item/view/target.geo" .}}
