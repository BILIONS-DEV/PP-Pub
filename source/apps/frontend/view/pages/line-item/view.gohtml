{{- /*gotype: source/apps/frontend/controller.AssignBidderEdit*/ -}}
{{$DemandLang := .Schema.LANG.Pages.LineItem}}
<div class="px-5 py-3 border-bottom border-gray-200">
    <div class="container-fluid px-0">
        <div class="row align-items-center">
            <div class="col">
                <a href="/line-item">
                    <span class="text-uppercase tiny text-gray-600 Montserrat-font font-weight-semibold">{{$DemandLang.Title}}</span>
                </a>
                <h1 class="h4 mb-0">{{$DemandLang.Edit}}</h1>
            </div>
        </div>
    </div>
</div>
{{/*End Header*/}}

<div class="px-5 pt-5 pb-3 height-main">
    <div class="container-fluid px-0">
        <div class="row">
            <div class="col-12">
                <form id="EditBidder">
                    <input type="hidden" name="id" value="{{.Row.TableLineItem.Id}}">
                    <div id="MainBox" class="card rounded-0 shadow-dark-80 border border-gray-50 mb-5">
                        <div class="d-flex align-items-center bg-dominant px-4 toggle-collapse">
                            <h5 class="card-header-title font-weight-semibold m-2">{{$DemandLang.Main}}</h5>
                            <div class="ms-auto pe-md-2">
                                <div class="export-dropdown">
                                    <a aria-controls="collapseMainBox" data-bs-target="#collapseMainBox"
                                       data-bs-toggle="collapse"
                                       aria-expanded="{{if (InArray "collapseMainBox" $.ListBoxCollapse false)}}false{{else}}true{{end}}"
                                       class="btn btn-link px-3 {{if (InArray "collapseMainBox" $.ListBoxCollapse false)}}{{else}}collapsed{{end}}"
                                       href="javascript:void(0);">
                                        <svg class="ms-2" xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                             viewBox="0 0 13 13">
                                            <rect data-name="Icons/Tabler/Chevron Down background" width="16"
                                                  height="16" fill="none"></rect>
                                            <path d="M.214.212a.738.738,0,0,1,.952-.07l.082.07L7.1,5.989a.716.716,0,0,1,.071.94L7.1,7.011l-5.85,5.778a.738.738,0,0,1-1.034,0,.716.716,0,0,1-.071-.94l.071-.081L5.547,6.5.214,1.233A.716.716,0,0,1,.143.293Z"
                                                  transform="translate(13 3.25) rotate(90)" fill="#1e1e1e"></path>
                                        </svg>
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div id="collapseMainBox"
                             class="collapse {{if (InArray "collapseMainBox" $.ListBoxCollapse false)}}{{else}}show{{end}}"
                             data-bs-parent="#MainBox">
                            <div class="card-body px-5 py-for-card">
                                <div class="row my-4">
                                    <div class="d-flex align-items-center">
                                        <label class="col-form-label form-label form-label-lg w-input-group"
                                               for="name">
                                            {{$DemandLang.LineItemName}}
                                        </label>
                                        <input type="text" class="form-control w-50-custom" disabled
                                               id="name" name="name" value="{{.Row.TableLineItem.Name}}">
                                        <span class="form-text ms-2 w-auto invalid-feedback"></span>
                                    </div>
                                </div>
                                {{/*Line Item Name*/}}

                                <div class="row my-4">
                                    <div class="d-flex">
                                        <label class="col-form-label form-label form-label-lg w-input-group"
                                               for="description">
                                            {{$DemandLang.Description}}
                                        </label>
                                        <textarea id="description" name="description" disabled
                                                  class="form-control w-50-custom">{{.Row.TableLineItem.Description}}</textarea>
                                        <span class="form-text ms-2 w-auto invalid-feedback"></span>
                                    </div>
                                </div>
                                {{/*Description*/}}

                                <div class="row my-4">
                                    <div class="d-flex align-items-center">
                                        <label class="col-form-label form-label form-label-lg w-input-group"
                                               for="server_type">
                                            {{$DemandLang.Type}}
                                        </label>
                                        <select id="server_type" name="server_type" class="form-select w-50-custom"
                                                data-placeholder="Select a type" disabled>
                                            <option value="1"
                                                    {{if eq .Row.TableLineItem.ServerType.Int 1}}selected{{end}}>Prebid
                                            </option>
                                            <option value="2"
                                                    {{if eq .Row.TableLineItem.ServerType.Int 2}}selected{{end}}>Google
                                            </option>
                                        </select>
                                        <span class="form-text ms-2 w-auto invalid-feedback"></span>
                                    </div>
                                </div>
                                {{/*Bidder Type*/}}

                                <div class="row my-4 box_type_google_show {{if eq .Row.TableLineItem.ServerType.Int 1}}d-none{{end}}">
                                    <div class="d-flex">
                                        <label class="col-form-label form-label form-label-lg w-input-group"
                                               for="select_account">
                                            {{$DemandLang.SelectAccount}}
                                        </label>
                                        <div class="box-select2 w-50-custom">
                                            <select id="select_account" name="select_account"
                                                    class="form-select select2"
                                                    data-placeholder="Select Account"
                                                    style="text-transform: capitalize" disabled>
                                                <option></option>
                                                {{range .ListAccountGoogle}}
                                                    <option class="h4" value="{{.TableBidder.Id}}"
                                                            {{if eq .TableBidder.Id $.AccountGoogleSelected.TableBidder.Id}}selected{{end}}>{{.TableBidder.DisplayName}}</option>
                                                {{end}}
                                            </select>
                                        </div>
                                        <span class="form-text ms-2 w-auto invalid-feedback"></span>
                                    </div>
                                </div>
                                {{/*Select account*/}}

                                <div class="row my-4 box_type_google_show {{if eq .Row.TableLineItem.ServerType.Int 1}}d-none{{end}}">
                                    <div class="d-flex align-items-center">
                                        <label class="col-form-label form-label form-label-lg w-input-group">
                                            {{$DemandLang.LinkedGam}}
                                        </label>
                                        <div class="box-select2 w-50-custom">
                                            <select id="linked_gam" name="linked_gam" disabled
                                                    class="form-select select2"
                                                    data-placeholder="Select a GAM">
                                                <option></option>
                                                {{range .GamNetworks}}
                                                    <option value="{{.TableGamNetwork.Id}}"
                                                            {{if eq .TableGamNetwork.Id $.Row.TableLineItem.LinkedGam}}selected{{end}}>{{.TableGamNetwork.NetworkName}}</option>
                                                {{end}}
                                            </select>
                                        </div>
                                        <span class="form-text ms-2 w-auto invalid-feedback"></span>
                                    </div>
                                </div>
                                {{/*Linked GAM*/}}

                                <div class="row my-4">
                                    <div class="d-flex">
                                        <label class="col-form-label form-label form-label-lg w-input-group">
                                            {{$DemandLang.Priority}}
                                        </label>
                                        <select id="priority" name="priority" class="form-select w-50-custom" disabled>
                                            <option {{if eq 1 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="1">1
                                            </option>
                                            <option {{if eq 2 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="2">2
                                            </option>
                                            <option {{if eq 3 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="3">3
                                            </option>
                                            <option {{if eq 4 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="4">4
                                            </option>
                                            <option {{if eq 5 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="5">5
                                            </option>
                                            <option {{if eq 6 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="6">6
                                            </option>
                                            <option {{if eq 7 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="7">7
                                            </option>
                                            <option {{if eq 8 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="8">8
                                            </option>
                                            <option {{if eq 9 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="9">9
                                            </option>
                                            <option {{if eq 10 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="10">10
                                            </option>
                                            <option {{if eq 11 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="11">11
                                            </option>
                                            <option {{if eq 12 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="12">12
                                            </option>
                                            <option {{if eq 13 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="13">13
                                            </option>
                                            <option {{if eq 14 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="14">14
                                            </option>
                                            <option {{if eq 15 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="15">15
                                            </option>
                                            <option {{if eq 16 .Row.TableLineItem.Priority}}selected{{end}}
                                                    value="16">16
                                            </option>
                                        </select>
                                    </div>
                                </div>
                                {{/*Priority*/}}
                                {{if eq .Row.TableLineItem.IsLock 2}}
                                    <fieldset id="fieldSetStatus">
                                        <div class="row my-4">
                                            <div class="d-flex align-items-center">
                                                <label class="col-form-label form-label form-label-lg w-input-group">
                                                    {{$DemandLang.Status}}
                                                </label>
                                                <div class="form-check form-switch mb-0 ps-0"
                                                     style="cursor: not-allowed!important;">
                                                    <input type="checkbox" data-toggle="toggle" class=""
                                                           {{if eq 1 .Row.TableLineItem.Status }}checked{{end}}
                                                           data-on="running" data-off="paused"
                                                           data-onstyle="dominant btn-sm"
                                                           data-offstyle="soft-secondary btn-sm"
                                                           data-style="rounded-24"
                                                           id="status" name="status">
                                                </div>
                                            </div>
                                        </div>
                                    </fieldset>
                                {{else}}
                                    <div class="row my-4">
                                        <div class="d-flex align-items-center">
                                            <label class="col-form-label form-label form-label-lg w-input-group">
                                                {{$DemandLang.Status}}
                                            </label>
                                            <div class="form-check form-switch mb-0 ps-0"
                                                 style="cursor: not-allowed!important;">
                                                <input type="checkbox" data-toggle="toggle" class=""
                                                       {{if eq 1 .Row.TableLineItem.Status }}checked{{end}}
                                                       data-on="running" data-off="paused"
                                                       data-onstyle="dominant btn-sm"
                                                       data-offstyle="soft-secondary btn-sm"
                                                       data-style="rounded-24"
                                                       id="status" name="status">
                                            </div>
                                        </div>
                                    </div>
                                {{end}}
                                {{/*Status*/}}
                            </div>
                        </div>
                    </div>
                    {{/*Main Card*/}}

                    <fieldset id="myFieldset">
                        <div id="TargetingBox" class="card rounded-0 shadow-dark-80 border border-gray-50 mb-5">
                            <div class="d-flex align-items-center bg-dominant px-4 toggle-collapse">
                                <h5 class="card-header-title font-weight-semibold m-2">{{$DemandLang.Targeting}}</h5>
                                <div class="ms-auto pe-md-2">
                                    <div class="export-dropdown">
                                        <a aria-controls="collapseTargetingBox" data-bs-target="#collapseTargetingBox"
                                           data-bs-toggle="collapse"
                                           aria-expanded="{{if (InArray "collapseTargetingBox" $.ListBoxCollapse false)}}false{{else}}true{{end}}"
                                           class="btn btn-link px-3 {{if (InArray "collapseTargetingBox" $.ListBoxCollapse false)}}{{else}}collapsed{{end}}"
                                           href="javascript:void(0);">
                                            <svg class="ms-2" xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                                 viewBox="0 0 13 13">
                                                <rect data-name="Icons/Tabler/Chevron Down background" width="16"
                                                      height="16" fill="none"></rect>
                                                <path d="M.214.212a.738.738,0,0,1,.952-.07l.082.07L7.1,5.989a.716.716,0,0,1,.071.94L7.1,7.011l-5.85,5.778a.738.738,0,0,1-1.034,0,.716.716,0,0,1-.071-.94l.071-.081L5.547,6.5.214,1.233A.716.716,0,0,1,.143.293Z"
                                                      transform="translate(13 3.25) rotate(90)" fill="#1e1e1e"></path>
                                            </svg>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div id="collapseTargetingBox"
                                 class="collapse {{if (InArray "collapseTargetingBox" $.ListBoxCollapse false)}}{{else}}show{{end}}"
                                 data-bs-parent="#TargetingBox">
                                <div class="card-body px-5 py-for-card">
