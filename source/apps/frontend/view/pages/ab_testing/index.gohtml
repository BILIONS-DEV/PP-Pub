{{template "ab_testing/index/inc.header" .}}
{{/*End Header*/}}

<div class="pubpower-main pb-3 height-main">
    <div class="container-fluid px-0">
        <div class="row">
            <div class="col-12 mb-4">
                <div class="card">
                    <div class="align-items-center p-0 pb-4 col-md-12">
                        <div class="ms-auto col-md-12 filter p-0">
                            {{template "ab_testing/index/inc.filter" .}}
                        </div>
                    </div>
                    <div class="table-responsive" style="height:65vh">
                        <table id="tableAbTestings" class="table-nowrap table-list w-100 dataTable no-footer">
                            <thead>
                            <tr>
                                <th style="max-width:2%" class="fw-bold" data-obj-field="id"><strong>ID</strong></th>
                                <th style="max-width:350px" class="fw-bold" data-obj-field="name"><strong>Name</strong></th>
{{/*                                <th class="fw-bold" data-obj-field="description"><strong>Description</strong></th>*/}}
                                <th class="fw-bold" data-obj-field="test_type"><strong>Test Type</strong></th>
                                <th class="fw-bold" data-obj-field="bidder"><strong>Bidder</strong></th>
                                <th class="fw-bold" data-obj-field="test_group_size"><strong>Test Group Size</strong></th>
                                <th class="fw-bold" data-obj-field="status"><strong>Status</strong></th>
                                <th class="fw-bold" data-obj-field="action" style="max-width:130px" data-orderable="false"><strong>Action</strong></th>
                            </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
