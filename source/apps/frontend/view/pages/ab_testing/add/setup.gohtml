{{- /*gotype: source/apps/frontend/controller.AssignAbTestingAdd*/ -}}
{{$Lang := .Schema.LANG.Pages.AbTesting}}
<div class="pp15">
    <div class="pp5">{{$Lang.Name}}</div>
</div>
<div class="pp7">
    <div class="pp-9">
        <input class="pp-10 form-control" type="text" id="name" name="name" value="">
        <span class="form-text ms-2 w-auto invalid-feedback"></span>
    </div>
</div>
<div class="pp15">
    <div class="pp5"> {{$Lang.Description}}</div>
</div>
<div class="pp7">
    <textarea class="pp17" spellcheck="false" id="description" name="description"></textarea>
    <span class="form-text ms-2 w-auto invalid-feedback"></span>
</div>
<div class="pp15">
    <div class="pp5">{{$Lang.TestType}}</div>
</div>
<div class=" cus-selectpicker pp-9">
    <select id="test_type" name="test_type" class="form-control selectpicker">
{{/*        <option value="1">Auction Timeout</option>*/}}
{{/*        <option value="2">Client vs Server</option>*/}}
{{/*        <option value="3">User Id Module</option>*/}}
        <option value="4" selected>Dynamic vs Hard Price Floor</option>
    </select>
    <span class="form-text ms-2 w-auto invalid-feedback"></span>
</div>
<div class="pp15">
    <div class="pp5">Status</div>
</div>
<div class="pp7">
    <label class="switch"> <input type="checkbox" id="status" name="status" checked> <span class="slider round"></span> </label>
</div>